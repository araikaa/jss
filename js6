  <script type="text/javascript">
function  LeapYear(year) {
    if ((0 == year % 4) && (0 != year % 100) || (0 == year % 400)) {
        console.log('leap year');
    } else {
        console.log('not leap year');
    }
}
const year = prompt('Enter a year:');
LeapYear(year);
 
